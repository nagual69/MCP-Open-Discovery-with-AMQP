{
  "name": "mcp-open-discovery",
  "version": "2.0.0",
  "description": "Networking Discovery tools exposed via Model Context Protocol - SDK Compatible",
  "main": "mcp_open_discovery_server.js",
  "scripts": {
    "start": "node mcp_open_discovery_server.js",
    "start-stdio": "set TRANSPORT_MODE=stdio&& node mcp_open_discovery_server.js",
    "start-http": "set TRANSPORT_MODE=http&& node mcp_open_discovery_server.js",
    "start-amqp": "set TRANSPORT_MODE=amqp&& node mcp_open_discovery_server.js",
    "start-http-amqp": "set TRANSPORT_MODE=http,amqp&& node mcp_open_discovery_server.js",
    "start-all": "set TRANSPORT_MODE=stdio,http,amqp&& node mcp_open_discovery_server.js",
    "start-both": "set TRANSPORT_MODE=both&& node mcp_open_discovery_server.js",
    "start-oauth": "set TRANSPORT_MODE=http&& set OAUTH_ENABLED=true&& node mcp_open_discovery_server.js",
    "test": "node test_sdk_server.js",
    "test-container": "node testing/test_container_health.js",
    "test-tools": "node testing/test_container_tools.js",
    "test-http": "node testing/test_http_transport.js",
    "test-amqp": "node testing/test-amqp-transport.js",
  "test-oauth": "node testing/test_oauth_implementation.js",
  "ui:test": "node testing/run_management_ui.js",
  "ui:serve": "node testing/run_management_ui.js --serve",
  "test:plugin-validation": "node testing/test_plugin_validation.js",
  "test-registry": "node testing/test_registry_register_from_db.js && node testing/test_registry_reregister_hot_reload.js",
    "health": "powershell -Command \"Invoke-WebRequest -Uri http://localhost:3000/health -UseBasicParsing\"",
    "migrate-credentials": "node tools/migrate_proxmox_credentials.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nagual69/mcp-open-discovery.git"
  },
  "keywords": [
    "mcp",
    "busybox",
    "networking",
    "tools",
    "container"
  ],
  "author": "Toby Schmeling",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nagual69/mcp-open-discovery/issues"
  },
  "homepage": "https://github.com/nagual69/mcp-open-discovery#readme",
  "engines": {
    "node": ">=23"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/amqplib": "^0.10.7",
    "amqplib": "^0.10.8",
  "adm-zip": "^0.5.10",
    "axios": "^1.11.0",
    "chokidar": "^3.6.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "mcp-types": "^1.0.2",
    "net-snmp": "^3.10.1",
    "node-fetch": "^2.7.0",
    "sqlite3": "^5.1.7",
    "ws": "^8.18.3",
    "zod": "^3.25.64",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "json-schema-to-zod": "^2.6.1"
  }
}
